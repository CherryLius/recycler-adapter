<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cherry.android.recycler.sample">

    <application
        android:name=".LeakApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".linear.LinearActivity" />
        <activity android:name=".linear.LinearVerticalActivity" />
        <activity android:name=".linear.LinearHorizontalActivity" />
        <activity android:name=".linear.LinearLoadingMoreActivity" />
        <activity android:name=".linear.SecondActivity" />
        <activity android:name=".linear.ThirdActivity" />
    </application>

</manifest>